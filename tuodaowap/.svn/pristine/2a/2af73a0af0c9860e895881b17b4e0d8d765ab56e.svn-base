 body{background:url(../images/newblies/body_bg.png);font-family:"Microsoft YaHei";}
 input[type=button], input[type=submit], input[type=file], button {-webkit-appearance: none; } 
.dis_none{display:none;}
.header{overflow: hidden;}
.header .log{width:1.6rem;height:0.75rem;background: url(../images/newblies/log.png) no-repeat;background-size: 100%;margin:0.32rem 0 0 0.4rem;}
.header .txt{text-align: center;font-family:"宋体";}
.header .txt .txt_top{font-size:0.5rem;color:#fff;margin-top:0.13rem;}
.header .txt .txt_top .iconfont{font-size:1rem;}
.header .txt .txt_bot{font-size:0.3rem;color:#fff;margin-top:0.3rem;}
.header .txt .txt_bot span{font-size:0.46rem;}
.regis{width:6.9rem;height:5.72rem;margin:0.5rem auto;background: url(../images/newblies/red_packet2.png) no-repeat;background-size: 100%;overflow: hidden;}
.span_btn{display:block;width:4.95rem;height:0.72rem;font-size:0.28rem;color:#fff;text-align:center;line-height: 0.72rem;background-color:#ff8311;margin:4.4rem auto;border-radius: 5px;}
.span_btn span{font-size:0.3rem;}
.div_puclic{width:6.9rem;background-color:#fff;border-radius: 5px;margin:0.4rem auto;overflow: hidden;}
.text_top{font-size:0.3rem;color:#ff8311;text-align:center;margin:0.4rem 0;font-weight: bolder;}
.invest .invest_cont{width:5.83rem;height:1.75rem;margin:0 auto;background: url(../images/newblies/interest.png);background-size:100%;}
.invest .span_btn{margin:0.4rem auto 0.6rem;}
.aptitude .text_cont{width:100%;overflow: hidden;padding-bottom:0.6rem;position:relative;}
.aptitude .text_cont .border_x{position:absolute;left:0.4rem;right:0.4rem;top:3.3rem;display:block;height:0.01rem;width:6.1rem;border-top:1px solid #eee;}
.aptitude .text_cont .border_y{position:absolute;left:50%;top:0.3rem;display:block;width:0.01rem;height:5.9rem;border-left:1px solid #eee;}
.aptitude .text_cont dl{width:50%;font-size:0.24rem;float:left;text-align:center;margin-top:0.16rem;}
.aptitude .text_cont dl.thre{margin-top:0.8rem;}
.aptitude .text_cont dl.four{margin-top:0.8rem;}
.aptitude .text_cont dl dt{width:1.32rem;height:1.32rem;margin:0 auto;background:url(../images/newblies/icon.png) no-repeat;background-size:400% 106%;}
.aptitude .text_cont dl .financing{background-position:0rem 0rem;}
.aptitude .text_cont dl .Management{background-position:-1.32rem 0rem;}
.aptitude .text_cont dl .tuodao{background-position:-2.64rem 0rem;}
.aptitude .text_cont dl .car{background-position:-3.96rem 0rem;}
.aptitude .text_cont dl h3{font-size:0.24rem;margin:0.2rem 0;color:#333;}
.aptitude .text_cont dl p{font-size:0.22rem;color:#666;}
.introduce{text-align:center;overflow:hidden;}
.introduce_txt{margin-top:0.15rem;}
.introduce p{font-size:0.22rem;color:#333;line-height:0.32rem;}
.introduce_bg{margin:0.4rem auto;width:5.65rem;height:5.85rem;background:url(../images/newblies/introduce_bg.png) no-repeat;background-size:100%;}
.introduce .span_btn{margin:0.8rem auto 0.6rem;}
.footer{font-size:0.22rem;color:#fff;text-align:center;margin:0.8rem 0;}
.footer p{line-height:0.4rem;}
.regis_phone{width:6.9rem;height:5.6rem;margin:0.6rem auto 0.4rem;padding-bottom:0.8rem;text-align:center;background:url(../images/newblies/red_packet1.png) no-repeat;background-size:100%;overflow: hidden;}
.regis_phone .span_btn{margin:0.3rem auto 0;}
.regis_phone ul{margin-top:1.88rem;padding:0 0.9rem;}
.regis_phone li{width:100%;height:0.72rem;overflow: hidden;margin-top:0.3rem;}
.regis_phone li:nth-child(1){margin-top:0;}
.regis_phone input{padding-left:0.22rem;float:left;width:4.85rem;height:0.7rem;line-height:0.7rem;border-radius: 5px;font-size:0.3rem;color:#333;border:none;}
input::-webkit-input-placeholder{color:#bebebe;}
input::-webkit-input-placeholder {color: #bebebe; } 
input:-moz-placeholder {color: #bebebe; } 
input::-moz-placeholder {color: #bebebe; } 
input:-ms-input-placeholder {color: #bebebe; }
.regis_phone .num{width:2.7rem;}
.regis_phone .num_btn{float:left;padding-left:0;width:1.94rem;height:0.7rem;text-align:center;line-height:0.7rem;border-radius:3px;margin-left:0.2rem;background-color:#fa8910;font-size:0.26rem;color:#fff;}
.regis_phone .dis_btn{background-color:#d1d1d1;}
.regis_phone input.dis_btn:disabled {background-color: rgba(209,209,209, 1);opacity: 1;}
.import_phone{width:6.9rem;height:4.62rem;background:url(../images/newblies/red_packet3.png) no-repeat;background-size:100%;margin:0.6rem auto;text-align:center;}
.import_phone .span_btn{margin:0.3rem auto 0;}
.import_phone input{margin-top:1.88rem ;width:4.74rem;height:0.7rem;font-size:0.3rem;border:none;border-radius: 5px;padding-left:0.2rem;color:#333;}

