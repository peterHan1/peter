html, body, div, ul, li, h1, h2, h3, h4, h5, h6, p, dl, dt, dd, ol, form, input, textarea, th, td, select {margin: 0;padding: 0;}
*{font-family: "Microsoft Yahei";}
h1, h2, h3, h4, h5, h6{font-weight:normal;}
ul,ol {list-style: none;}
img {border: none;vertical-align: middle;}
a {text-decoration: none;}

body{ font-size:14px;font-family: "Microsoft YaHei";}
.fl {float: left}
.fr {float: right}
.tl {text-align: left;}
.tc {text-align: center}
.tr {text-align: right;}
.dis_no{display:none;}
.font_red{color:red;}
.txt_l{text-align: left;}
.txt_c{text-align: center;}
.txt_r{text-align: right;}
/*登录*/
.login .td_log{margin-top:2rem;text-align:center;}
.login form{width:3.2rem;margin:0 auto;}
.login ul{height:2rem;}
.login li{width:100%;height:0.8rem;line-height:0.78rem;margin-top:0.2rem;font-size:0.4rem;background-color: #F2F2F2;color:#999;}
.login li input{height:0.5rem;width:2.8rem;margin-top:0;border:none;font-size:0.35rem;background-color: #F2F2F2;}
.login .sub {font-size:0.4rem;}
.login .sub input{height:0.7rem;width:3rem;font-size:0.3rem;}
.layui-m-layer1 .login_show .layui-m-layercont{text-align:center;}
.layui-m-layersection .login_show{width:3rem;height:2.5rem;font-size:0.3rem;}
.layui-m-layersection .login_show h3{font-size:0.4rem;}
.login_show .layui-m-layercont {line-height:0.8rem;}
.login_show .layui-m-layerbtn span[yes]{color:gray;font-size:0.45rem;background-color:white;border:none;}
/*搜索*/
.search{height:1rem;font-size:0.3rem;line-height:1rem;background-color: rgb(0, 0, 0);opacity: 0.3;filter: alpha(opacity=50);}
.search input{height:0.8rem;width:90%;margin:0 auto;font-size:0.3rem;border:none;border-radius: 5px;margin:1.5% 5%;}
.search_show{height:100%;margin-top:0;}
.search_layer{width: 100%;height: 100%;background-color: rgb(0, 0, 0);opacity: 0.3;filter: alpha(opacity=50);z-index: 9999;}
.sea_can{color:white;}
/*菜单*/
.box{width:100%;height:100%;display: -webkit-box;display: -moz-box;position: relative;overflow: hidden;}
#right_cont{width:70%;height:100%;background-color: #F2F2F2;}
#right_cont .index_ok{margin-top: 50%}
#right_cont .index_ok .index_img{height:2rem;width:100%;background:url(../img/ok.png)no-repeat; background-size: 100%;}
#left_ment{background-color:#F2F2F2;width:30%;height:100%;border-right:1px solid gray;}
.left_ment .uc{font-size:0.3rem;height:1.5rem;}
.left_ment .uc a{display:block;width:100%;height:100%;background-color:white;color:black;}
.left_ment .uc a span{display:block;width:1rem;height:1rem;background:url(../img/uc.png) no-repeat;background-size:100% ;}
.left_ment li div,dd{height:1rem;line-height:1rem;font-size:0.4rem;}
.left_ment li dd a{display:block;width:100%;color:black;background-color:white;border-bottom:1px solid gray;}
.left_ment li .on{background-color:#FF9900;color:white;}

.top{width:100%;height:1.5rem;line-height:1.5rem;font-size:0.3rem;border-bottom:1px solid gray;font-size:0.4rem;text-align:center;}
.top h2{font-size:0.4rem;}
/*借款人列表*/
#add_borrow{width:200%;height:100%;position:absolute;top:100%;left:0;font-size:0.25rem;overflow: hidden;z-index:99;}
.borrower{height:100%;width:100%;overflow: hidden;}
.borrower .borrower_tab{height:1rem;font-size:0.3rem;line-height:1rem;border-bottom:1px solid gray;}
.borrower .borrower_tab li {width:33.3%;text-align:center;height:1rem;}
.borrower .borrower_tab li a{display:block;color:gray;box-sizing: border-box;}
.borrower .borrower_tab .li_on{height:1rem;color:#FF9900;border-bottom:0.05rem solid #FF9900;}
.borrower .tabcontent li{height:1rem;font-size:0.28rem;}
.borrower .tabcontent li .cont_fl{}
.borrower .tabcontent li .cont_fr{line-height:1rem;}
.borrower .tabcontent li .cont_fr .font_gray{color:gray;}
.borrower .add_borrower{width:50%;height:100%;background-color:white;}
.borrower .add_borrower .add_title{width:100%;height:0.8rem;text-align: center;line-height:0.8rem;}
.borrower .add_borrower .add_cont li{width:100%;height:0.5rem;border-bottom:1px solid gray;}
.borrower .add_borrower .add_cont li a{display:block;width:100%;height:100%;}
.borrower .add_borrower .add_cont li input{width:100%;height:0.5rem;border:none;font-size:0.3rem;}

.borrower .add_borrower .add_cont_bot .border_no{border:none;}

/*选择银行*/
.select_bank{width:50%;height:100%;background-color:white;}
.select_bank .bank_title{width:100%;height:1rem;text-align:center;font-size:0.3rem;line-height: 1rem;}
.ul_bank ul li{border-bottom:1px solid #d8d8d8;width:94%;position:relative;padding:1% 3%;background-color:white;overflow: hidden;}
.ul_bank ul li h4{height:0.7rem;float:left;line-height:1rem;color:#333;font-weight: bold;}
.ul_bank ul li span{display:inline-block;width:1rem;height:1rem;float:left;margin-right:0.2rem;background:url(../img/bank_logo.png) no-repeat;background-size:5rem 4rem;}
.ul_bank{width:100%;height:100%;overflow: hidden}
.ul_bank .bank_on{background-color:#E4E4E4;}

#bank_ul{width:100%;z-index: 999;}
#bank_ul .ICBC{background-position:0.0rem 0rem;}
#bank_ul .CCB{background-position:-1rem 0rem;}
#bank_ul .BOC{background-position:-2rem 0rem;}
#bank_ul .ABC{background-position:-3rem 0rem;}
#bank_ul .CMB{background-position:-4rem 0rem;}
#bank_ul .CITIC{background-position:0rem -1rem;}
#bank_ul .SPDB{background-position:-1rem -1rem;}
#bank_ul .CIB{background-position:-2rem -1rem;}
#bank_ul .CMBC{background-position:-3rem -1rem;}
#bank_ul .CEB{background-position:-4rem -1rem;}
#bank_ul .PAB{background-position:0rem -2rem;}
#bank_ul .HXB{background-position:-1rem -2rem;}
#bank_ul .BOB{background-position:-2rem -2rem;}
#bank_ul .CGB{background-position:-3rem -2rem;}
#bank_ul .NBCB{background-position:-4rem -2rem;}
#bank_ul .HZB{background-position:0rem -3rem;}
#bank_ul .BOS{background-position:-1rem -3rem;}
#bank_ul .PSBC{background-position:-2rem -3rem;}
#bank_ul .BCM{background-position:-3rem -3rem;}

/*创建合同*/
.top ul li{float:left;width:50%;}
.top ul .li_on{height:1.45rem;color: #FF9900;border-bottom: 0.05rem solid ##FF9900;}
.establish_ul{width:85%;height:100%;}
.est_contTop{height:0.8rem;width:100%;font-size:0.25rem;}
.est_contTop ul{width:100%;height:100%;border-bottom: 1px solid gray;display:-moz-box;display:-webkit-box;display:-ms-flexbox;display:box;}
.est_contTop li{line-height:0.8rem;font-size:0.25rem;text-align: center;-webkit-box-flex: 1;-moz-box-flex: 1;-ms-box-flex: 1;box-flex: 1;}
.est_contTop li.on{height:0.75rem;color:#FF9900;border-bottom: 0.05rem solid ##FF9900;}
.est_conts .est_contBot li{width:100%;height:1rem;border-bottom: 1px solid gray;font-size:0.24rem;overflow: hidden;}
.est_conts .est_cont{white-space: nowrap;height:100%;clear: both;}
.est_conts .est_cont div{height:100%;}
.est_conts .est_contL div{height:100%;float:left;}
.est_conts .est_contL div:nth-child(1){width:25%;}
.est_conts .est_contL div:nth-child(2){width:30%;}
.est_conts .est_contL div:nth-child(3){width:35%;}
.est_conts .est_contL div:nth-child(4){width:10%;}
.est_conts .est_contR{width:100px;background-color:red;text-align: center;line-height:1rem;color:white;}

/*信息*/
.uc_box{font-size:0.24rem;height:100%;width:100%;}
.uc_box p{height:0.8rem;line-height:0.8rem;margin-left:0.2rem;}
.uc_box ul li{padding:0 2%;width:96%;height:0.8rem;border-bottom:1px solid gray;line-height:0.8rem;background-color:white;}
.uc_box .phone input{height:90%;width:40%;border:none;font-size:0.24rem;}
.uc_box .phone .btn{width:100%;height:1rem;margin-top:0.3rem;line-height:1rem;text-align:center;}
.uc_box .phone .btn button{width:80%;height:0.5rem;background-color:rgb(201,201,201);border-radius: 5px;border:none;font-size:0.24rem;color:white;}




