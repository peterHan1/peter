<script>
	$(function(){
		$(".num_btn").click(function(){
			wait=60;
			time($(this));
		});
		function time(o) {
		      if (wait == 0) {
		      		o.removeAttr('disabled')
		            o.val("重新获取");
		            o.removeClass('dis_btn');
		            
		        }else{
		        	o.attr('disabled','disabled')
		            o.val("重新发送(" + wait + ")");
		            o.addClass('dis_btn');
		            wait--;
		            setTimeout(function() {
		                time(o)
		            },
		            1000)
		    }  

		    }
	})
</script>