$(function(){
	$(".establish_ul li").on('click',function(){
		var index = $(this).index();
		$(".establish_ul li").removeClass("li_on");
		$(this).addClass("li_on");
		$('.est_conts').eq(index).show(200).siblings('.est_conts').hide();
	})
	
	$(".est_contTop li").on('click',function(){
		var index = $(this).index();
		$(".est_contTop li").removeClass("on");
		$(this).addClass("on");
		$('.est_audit').eq(index).show(200).siblings('.est_audit').hide();		
	})
})
