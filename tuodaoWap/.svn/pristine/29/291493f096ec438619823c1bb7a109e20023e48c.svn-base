html,body{height:100%;background-color:#f2f3f7;vertical-align: baseline;user-select: none;-webkit-user-select: none;-moz-user-select: none;-ms-user-select: none;}
*{-webkit-overflow-scrolling: touch;box-sizing:border-box; font-family:"Microsoft YaHei";}
input,textarea{-webkit-appearance: normal; -moz-appearance: normal; -o-appearance: normal; appearance: normal;border:none;outline:none;-webkit-tap-highlight-color:transparent;background-color:#fff;}
.dis_none{display: none;}
.fl{float:left;}
.fr{float:right;}
/*借款首页*/
.loan_index{background-color:#f2f3f7;}
.loan_index .index_top{background-color:#ff7400;padding-bottom:1.06rem;}
.loan_index .index_top .top_bg{width:7.5rem;height:7.79rem;background:url(../img/loan/index_bg.png) no-repeat;background-size:100% 100%;}
.loan_index .index_top .top_com{width:6.54rem;height:3.91rem;background:url(../img/loan/adva_bg.png)no-repeat;background-size:100% 100%;margin:-0.66rem auto 0;}
.loan_index .index_flow{width:7.5rem;height:3.4rem;background:url(../img/loan/flow_bg.png) no-repeat;background-size:100% 100%;}
.loan_index .index_cond{width:7.5rem;height:2.35rem;background:url(../img/loan/cond_bg.png) no-repeat;background-size:100% 100%;margin-top:0.2rem;}
.loan_index .index_phone{font-size:0.2rem;color:#b6b5b6;text-align: center;padding:0.2rem 0 1.28rem 0;}
.loan_index .index_phone span{font-size:0.24rem;color:#FF7400;}
.loan_index .apply_btn{width:100%;line-height:0.88rem;text-align: center;color:#212a36;font-size:0.3rem;background-color:#f6c861;position: fixed;left:0;bottom:0;}

/*填写信息页面*/
.loan{background-color:#f2f3f7;position: relative;}
.loan_box{width:200%;overflow: hidden;transition: all 0.5s;-moz-transition: all 0.5s;-webkit-transition: all 0.5s;-o-transition: all 0.5s;position: absolute;top: 0;left: 0;}
.loan_list{width:50%;background-color:#f2f3f7;overflow: hidden;}
#loan_list{width: 100%;z-index: 99;}
.loan .top{width:100%;background-color:#f2f3f7;overflow: hidden;}
.loan .top ul{margin-top:0.2rem;}
.loan .loan_list li{width:100%;height:1.1rem;padding:0 0.3rem;position:relative;border-bottom:1px solid #e4e4e4;overflow:hidden;background-color:#fff;font-size:0.26rem;}
.loan .loan_list li.city{border-bottom:none;}
.loan .loan_list .nav a{font-size:0.36rem;color:#b6b5b6;}
.loan .loan_list .nav h3{display:block;}
.loan .top li:nth-child(3){border:none;}
.loan .loan_list li .div_top{height:0.4rem;overflow: hidden;}
.loan .loan_list li .p_top{line-height:0.5rem;font-size:0.24rem;color:#ff7400;display: ;margin-top:0.3rem;}
.loan .loan_list li .div_bot{height:0.66rem;line-height:0.66rem;}
.loan .loan_list li .div_bot input{width:100%;height:100%;font-size:0.28rem;color:#212a36;caret-color:#ff7400;}
.loan .loan_list li .div_bot .inp_com{background-color: transparent;border: none;color:#626262;text-align: left;width: 92%;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.loan .loan_list li .div_bot .inp_font{color:#212a36;}
.loan .loan_list li .div_bot div{position:absolute;right:0.3rem;top:0.6rem;padding:0.15rem;background:url(../img/cooperation/x.png) no-repeat;background-size:100% 100%;}
.loan .loan_list li .div_bot .code_inp{width:70%;float: left;}
.loan .loan_list li .div_bot .code_btn{font-size:0.24rem;line-height:0.6rem;float: right;color:#FF7400;}
.loan .loan_list li .tex_bot{height:0.96rem;margin-top:0.18rem;}
.loan .loan_list li .tex_bot textarea{width:100%;height:100%;resize: none;outline: none;border: none;font-size:0.28rem;color:#212a36;caret-color:#ff7400;}
.loan .loan_list li .p{width: 92%;float: left;color: #626262;font-size: 0.28rem;}
.loan .loan_list li .iconfont{float: right;font-size: 0.2rem;}
.loan .txt{line-height:0.6rem;font-size:0.24rem;color:#626262;padding:0 0.3rem;}
.loan .div_btn{width:100%;padding:0 0.3rem;margin:0.6rem 0 0.2rem;text-align: center;overflow: hidden;}
.loan .div_btn .btn{display:block;width:100%;height:0.8rem;font-size:0.4rem;line-height:0.8rem;text-align:center;border-radius:5px;background-color:#cacaca;color:#fff;cursor: pointer;}
.loan .div_btn .loadn_btn{background-color:#FF7400;}
.loan .div_btn .sub_btn{background-color:#d3b580;}
.loan .div_btn p{margin-top:0.16rem;font-size:0.2rem;color:#7e7e7e;text-align:center;}
.loan .div_btn p .phone{color:#ff7400;font-size:0.26rem;}
.loan .div_btn .log{margin-top:1.1rem;font-size: 0.18rem;color:#b6b5b6;}
.inp_bot input::-webkit-input-placeholder{color: #cacaca;}
.inp_bot input:-moz-placeholder{color:#cacaca;}
.inp_bot input::-moz-placeholder{color:#cacaca;}
.inp_bot input:-ms-input-placeholder{color:#cacaca;}
.inp_bot textarea::-webkit-input-placeholder {color: #cacaca;}
input::-webkit-input-placeholder{color: #626262;}
input:-moz-placeholder{color:#626262;}
input::-moz-placeholder{color:#626262;}
input:-ms-input-placeholder{color:#626262;}
.layui-m-layersection .layui-m-layer-phone_show{width:5.1rem;font-size: 0.3rem;background-color:#fff;}
.layui-m-layersection .layui-m-layer-phone_show p{line-height:1.46rem;text-align: center;font-size:0.3rem;color:#212a36;}
.layui-m-layer-phone_show .layui-m-layercont{padding:0;}
.layui-m-layer-code_show{padding:0 0.3rem;}
.layui-m-layer-code_show .layui-m-layercont{height:0.8rem;line-height:0.8rem;padding:0;}
.layui-m-layer-code_show .iconfont{float: left;font-size:0.42rem;color:#d7d8d9;margin-right:0.2rem;}
.layui-m-layer-code_show p{float: left;font-size:0.28rem;}

.phone_btn{width:100%;border-top:1px solid #e4e4e4;overflow: hidden;}
.phone_btn span{display: inline-block;width:45%;line-height:0.8rem;text-align:center;color:#626262;font-size:0.3rem;}
.phone_btn span:nth-child(1){border-right: 1px solid #e4e4e4;cursor:pointer}
.phone_btn span a{display: inline-block;width:100%;height:100%;color:#ff7400;}
/*申请弹窗*/
.layui-m-layer-mess_show{width:5.1rem;}
.layui-m-layer-mess_show .layui-m-layercont{padding:0.5rem 0.3rem;text-align:left;background-color:#fff;color:#626262;font-size:0.28rem;border-radius:3px 3px 0 0;}
.layui-m-layer-mess_show .layui-m-layercont .lay_txtp{line-height:0.5rem;}
.layui-m-layer-mess_show .layui-m-layerbtn{height:0.8rem;line-height:0.8rem;background-color:#fff;border-radius:0 0 3px 3px;}
.layui-m-layer-mess_show .layui-m-layerbtn span[yes]{font-size:0.28rem;color:#FF7400;}

/*车辆品牌选择、城市选择*/
.loan_box .nav{width:50%;height:1rem;line-height:1rem;padding:0 0.3rem;text-align:center;border-bottom:1px solid #EDEDED;z-index:999;position:absolute;top:0;background-color:#fff;}
.loan_box .loan_list .nav{width:100%;}
.loan_box .nav h3{display: inline-block;font-weight: normal;font-size:0.32rem;color:#626262;}
.loan_box .nav span{font-size:0.28rem;color:#626262;}
.loan_box .nav .iconfont{visibility: hidden;font-size:0.5rem;}
.car_list_box,.city_list_box{width:50%;height:100%;font-size:0.2rem;overflow: hidden;}
.car_list_box .car_list,.city_list_box .city_list{position: absolute;top: 1rem;bottom: 0;width: 50%;height:100%;z-index: 99;}
.car_list_box .car_list .cities .title {height: 28px;padding-left: 0.3rem;line-height: 28px;background-color: #f2f3f7;font-family: Helvetica;font-size: 0.3rem;color: #212a36;}
.car_list_box .car_list .cities ul li:last-child p{border-bottom: none;}
.car_list_box .car_list .cities .item{padding:0 0.3rem;background-color: #fff;z-index: 99999;}
.car_list_box .car_list .cities .item p{height: 44px;line-height: 44px;font-size: 0.28rem;color:#626262;border-bottom: 1px solid #e4e4e4;}
.car_list_box .car_list .cities .onli {background-color: #f8f8f8;}
.car_list_box .car_list .cities .li_ok{background:#fff url(../img/loan/li_ok.png) no-repeat;background-size: 0.29rem 0.22rem; background-position:90% center;}
.car_list_box .car_list .cities .item .name {display: block;position: relative;}
.car_list_box .car_list .cities .item .name:before,
.car_list_box .car_list .cities .item .name:after {display: block;position: absolute;border-top: 1px solid #e5e5e5;left: 0;width: 100%;content: ' ';}
.car_list_box .car_list .cities .item .name:before {display: none;top: 0;}
.car_list_box .car_list .cities .item .name:after {display: block;bottom: 0;}
.car_list_box .car_list .cities .item:active {background-color: #f0f0f0;}
.car_list_box .car_list .cities .item:last-child .name:after {display: none;}
.car_list_box .car_list .shortcut {position: absolute;width: 0.8rem;right: 0;font-family: Helvetica;z-index: 9999;}
/*.car_list_box .car_list .shortcut .item {height: 0.42rem;line-height:0.32rem;padding-left:0.2rem;padding-top:0.1rem;text-align: center;font-size: 0.32rem;color: #fa8919;}*/
.car_list_box .car_list .shortcut .item {height: 0.42rem;line-height:0.32rem;padding-left:0.2rem;padding-top:0.1rem;text-align: center;font-size: 0.28rem;color: #212a36;}
.car_list_box .car_list .shortcut .jump_tips {position: fixed;top: 45%;right: 36%;z-index: 100;display: none;width: 1.4rem;padding: 0.5rem 0rem;font-size: 0.5rem;color: #fff;text-align: center;line-height: 1;background-color: rgba(0, 0, 0, 0.8);}
.car_list_box .car_list .scroller_hook {padding-bottom: 1rem;}
.car_list_box .list_one,.car_list_box .list_two{width:100%;height:100%;}
#car_list_botul{height: 100%;width:100%;padding-bottom:1.5rem;position:fixed;top:1rem;overflow-y: auto;}
#car_list_botul ul .p_title{height: 28px;padding-left: 0.3rem;line-height: 28px;background-color: #f2f3f7; font-family: Helvetica;font-size: 0.26rem;color: #212a36;}
.car_list_box .list_two ul li {z-index: 99999;padding: 0 0.3rem;background-color: #fff;cursor:pointer;-webkit-appearance: none;-webkit-user-select: none;-webkit-tap-highlight-color: rgba(200,200,200,0);}
.car_list_box .list_two ul li p{height: 44px;line-height: 44px;font-size: 0.28rem;color:#626262;border-bottom: 1px solid #e4e4e4;}
.car_list_box .list_two ul li:last-child p{border-bottom: none;}
.car_list_box .list_two ul .onli{background-color: #ededed;}
.car_list_box .car_nav{width:100%;height:1rem;line-height:1rem;padding:0 2%;text-align:center;border-bottom:1px solid #EDEDED;z-index:999;position:fixed;top:0;background-color:#fff;}
.car_list_box .car_nav h3{display: inline-block;font-weight: normal;font-size:0.3rem;}
.car_list_box .car_nav span{font-size:0.28rem;color:#626262;}
.car_list_box .car_nav .get_car{transform:rotate(180deg);}
.city_list .city_nav{height:0.6rem;line-height: 0.6rem;border-bottom: 1px solid #e4e4e4;background-color:#f2f3f7;}
.city_list .city_nav div{width:50%;float:left;text-align:center;font-size:0.24rem;color:#212a36;}
.city_list .city{width:100%;height:100%;overflow: hidden;}
.city_list .city .city_left,.city_right{width:50%;float: left;height:100%;}
.city_list .city .city_left{width:50%;}
.city_list .city .city_right{position: relative;overflow-y:auto;}
#city_ul{position: absolute;width:100%;height:100%;background-color:#fff;}
.province_ul{padding-bottom:1.5rem;}
.province_ul li{height:0.8rem;line-height: 0.8rem;text-align:center;background-color:#f2f3f7;font-size:0.28rem;color:#626262;}
.province_ul li.onli{background-color:#fff}
.city_ul li{height:0.8rem;line-height: 0.8rem;text-align:center;background-color:#fff;border-bottom: 1px solid #e4e4e4;font-size:0.28rem;color:#626262;}
.city_ul li.li_ok{background:url(../img/loan/li_ok.png) no-repeat;background-size: 0.29rem 0.22rem; background-position:right center;}
.city_ul{padding:0 0.3rem 1.5rem;position: absolute;width:100%;background-color:#fff;}


/*提交结果页面*/
.loan_resule{height:100%;background-color:#f2f3f7;position: relative;}
.loan_resule .nav{line-height:0.8rem;background-color:#fff;text-align: center;padding:0 0.3rem;}
.loan_resule .nav h3{font-size:0.3rem;color:#626262;font-weight: normal;}
.loan_resule .nav a{font-size: 0.36rem;color: #b6b5b6;}
.loan_resule .loan_resTop{width:100%;height:2.5rem;background-color:#ff8016;text-align:center;overflow: hidden;padding-top:0.18rem;}
.loan_resule .loan_resTop p{font-size:0.32rem;color:#fff;margin-top:0.1rem;}
.loan_resule .loan_resTop i{font-size:0.82rem;color:#fff;}
.loan_resule .loan_resCom{padding:0.32rem 0.3rem 0.52rem;}
.loan_resule .loan_resCom p{line-height:0.4rem;font-size:0.24rem;color:#626262;text-align:center;}
.loan_resule .loan_resCom .lo p{line-height:1.25rem;}
.loan_resule .loan_resCom p span{font-size:0.26rem;color:#FF7400;}
.loan_resule .loan_resBtn{width:100%;padding:0 0.3rem;overflow: hidden;}
.loan_resule .loan_resBtn a{width:100%;float:left;display: inline-block;line-height:0.8rem;text-align:center;color:#fff;font-size:0.3rem;color:#fff;background-color:#FF7400;border-radius: 3px;}
.bot_log{position: absolute;left:37%;bottom: 0.4rem;}
.bot_log p{font-size:0.2rem;color:#b6b5b6;}
/*加载中*/
.spinner_box{position: absolute;top: 30%;left:33%;width:2.2rem;height:2.12rem;background-color:#f8f8f8;border:1px solid #e4e4e4;border-radius: 5px;}
.spinner {margin: 0.62rem auto;width: 0.85rem;height: 0.85rem;position: relative;}
.container1 > div, .container2 > div, .container3 > div {width: 0.2rem;height: 0.2rem;background-color: #7e7e7e;border-radius: 100%;position: absolute;-webkit-animation: bouncedelay 1.2s infinite ease-in-out;animation: bouncedelay 1.2s infinite ease-in-out;-webkit-animation-fill-mode: both;animation-fill-mode: both;}
.spinner .spinner-container {position: absolute;width: 100%;height: 100%;}
.container2 {-webkit-transform: rotateZ(45deg);transform: rotateZ(45deg);}
.container3 {-webkit-transform: rotateZ(90deg);transform: rotateZ(90deg);}
.circle1 { top: 0; left: 0; }
.circle2 { top: 0; right: 0; }
.circle3 { right: 0; bottom: 0; }
.circle4 { left: 0; bottom: 0; }
.container2 .circle1 {-webkit-animation-delay: -1.1s;animation-delay: -1.1s;}
.container3 .circle1 {-webkit-animation-delay: -1.0s;animation-delay: -1.0s;}
.container1 .circle2 {-webkit-animation-delay: -0.9s;animation-delay: -0.9s;}
.container2 .circle2 {-webkit-animation-delay: -0.8s;animation-delay: -0.8s;}
.container3 .circle2 {-webkit-animation-delay: -0.7s;animation-delay: -0.7s;}
.container1 .circle3 {-webkit-animation-delay: -0.6s;animation-delay: -0.6s;}
.container2 .circle3 {-webkit-animation-delay: -0.5s;animation-delay: -0.5s;}
.container3 .circle3 {-webkit-animation-delay: -0.4s;animation-delay: -0.4s;}
.container1 .circle4 {-webkit-animation-delay: -0.3s;animation-delay: -0.3s;}
.container2 .circle4 {-webkit-animation-delay: -0.2s;animation-delay: -0.2s;} 
.container3 .circle4 {-webkit-animation-delay: -0.1s;animation-delay: -0.1s;}
@-webkit-keyframes bouncedelay {0%, 80%, 100% { -webkit-transform: scale(0.0) }40% { -webkit-transform: scale(1.0) }}
@keyframes bouncedelay {0%, 80%, 100% {transform: scale(0.0);-webkit-transform: scale(0.0);} 40% {transform: scale(1.0);-webkit-transform: scale(1.0);}}