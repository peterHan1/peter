*{-webkit-overflow-scrolling: touch;box-sizing:border-box; font-family:"Microsoft YaHei";}
.coop{background-color:#171618;}
.coop .top{width:7.5rem;height:8.06rem;background:url(../img/cooperation/banner.png) no-repeat;background-size: 100% 100%;}
.coop .object{width:6.45rem;height:4.04rem;background:url(../img/cooperation/object.png) no-repeat;background-size: 100% 100%;margin:-1.4rem auto 0;}
.coop .sistema{margin-top:1.02rem;}
.coop .sistema .txt{width:6.45rem;margin:0 auto;}
.coop .sistema .tema_title{width:1.64rem;height:0.36rem;background:url(../img/cooperation/tema_title.png) no-repeat;background-size: 100% 100%;}
.coop .sistema .tema_banner{width:15rem;height:2.2rem;padding:0.9rem 0;}
.coop .sistema .tema_banner div{width:1.43rem;height:2.17rem;float: left;margin-left:0.65rem;}
.coop .sistema .tema_banner div:nth-child(1){background:url(../img/cooperation/img1.png) no-repeat;background-size:100% 100%;}
.coop .sistema .tema_banner div:nth-child(2){background:url(../img/cooperation/img2.png) no-repeat;background-size:100% 100%;}
.coop .sistema .tema_banner div:nth-child(3){background:url(../img/cooperation/img3.png) no-repeat;background-size:100% 100%;}
.coop .sistema .tema_banner div:nth-child(4){background:url(../img/cooperation/img4.png) no-repeat;background-size:100% 100%;}
.coop .sistema .tema_banner div:nth-child(5){background:url(../img/cooperation/img5.png) no-repeat;background-size:100% 100%;}
.coop .sistema .tema_banner div:nth-child(6){background:url(../img/cooperation/img6.png) no-repeat;background-size:100% 100%;}
.coop .sistema .tema_banner div:nth-child(7){background:url(../img/cooperation/img7.png) no-repeat;background-size:100% 100%;}
.coop .sistema .tema_banner div:nth-child(8){background:url(../img/cooperation/img8.png) no-repeat;background-size:100% 100%;}
.coop .banner{overflow:auto;}
.coop .adva{width:6.45rem;height:6.41rem;background:url(../img/cooperation/adva.png) no-repeat;background-size:100% 100%;margin:0.35rem auto 0;}
.coop .flow{width:6.45rem;height:3.9rem;background:url(../img/cooperation/flow.png) no-repeat;background-size:100% 100%;margin:0.95rem auto 0;}
.coop .condition{width:6.45rem;height:5.12rem;background:url(../img/cooperation/condition.png) no-repeat;background-size:100% 100%;margin:1rem auto 0;}
.coop .phone{width:100%;height:0.98rem;line-height:0.98rem;margin:1rem 0 0.9rem 0;text-align:center;background-color:#211f22;font-size:0.2rem;color:#d3b580;}
.coop .phone span{font-size:0.26rem;text-decoration:underline}
.coop .btn{width:100%;height:0.9rem;line-height:0.9rem;text-align:center;font-size:0.3rem;background-color:#d3b580;font-weight: bold;position: fixed;left:0;bottom:0;z-index: 999;}
.coop .btn a{display: inline-block;width:100%;height:100%;color:#232124;}
.layui-m-layersection .layui-m-layer-phone_show{width:5.1rem;font-size: 0.3rem;background-color:#fff;}
.layui-m-layersection .layui-m-layer-phone_show p{line-height:1.46rem;text-align: center;}
.phone_btn{width:100%;border-top:1px solid #e4e4e4;overflow: hidden;}
.phone_btn span{display: inline-block;width:45%;line-height:0.8rem;text-align:center;}
.phone_btn span:nth-child(1){border-right: 1px solid #e4e4e4;cursor:pointer}
.phone_btn span a{display: inline-block;width:100%;height:100%;color:#ff7400;}
.dis_none{display: none;}
.layui-m-layer-phone_show .layui-m-layercont{padding:0;}


input,textarea{-webkit-appearance: normal; -moz-appearance: normal; -o-appearance: normal; appearance: normal;border:none;outline:none}
.dis_none{display:none;}
.div_bot input::-webkit-input-placeholder{color: #626262;}
.div_bot input:-moz-placeholder{color:#626262;}
.div_bot input::-moz-placeholder{color:#626262;}
.div_bot input:-ms-input-placeholder{color:#626262;}
.joinIn{background-color:#f2f3f7;overflow: hidden;}
.joinIn .top{width:100%;margin-top:0.2rem;}
.joinIn .top li{width:100%;height:1.1rem;padding:0 0.3rem;position:relative;border-bottom:1px solid #e4e4e4;overflow:hidden;background-color:#fff;font-size:0.26rem;}
.joinIn .top li:nth-child(3){height:1.6rem;border:none;}
.joinIn li .div_top{height:0.4rem;overflow: hidden;}
.joinIn li .p_top{line-height:0.5rem;font-size:0.22rem;color:#ff7400;display: none;}
.joinIn li .div_bot{height:0.66rem;line-height:0.66rem;}
.joinIn li .div_bot input{width:100%;height:100%;font-size:0.28rem;color:#212a36;caret-color:#ff7400;}
.joinIn li .div_bot div{position:absolute;right:0;top:0.4rem;width:0.24rem;height:0.24rem;margin:0.2rem;background:url(../img/cooperation/x.png) no-repeat;background-size:100% 100%;z-index: 999;}
.joinIn li .tex_bot{height:0.96rem;margin-top:0.18rem;}
.joinIn li .tex_bot textarea{width:100%;height:100%;resize: none;outline: none;border: none;font-size:0.28rem;color:#212a36;caret-color:#ff7400; font-family:;}
.joinIn .txt{line-height:0.6rem;font-size:0.2rem;color:#7e7e7e;padding:0 0.3rem;}
.inp_bot input::-webkit-input-placeholder{color: #cacaca;}
.inp_bot input:-moz-placeholder{color:#cacaca;}
.inp_bot input::-moz-placeholder{color:#cacaca;}
.inp_bot input:-ms-input-placeholder{color:#cacaca;}
.inp_bot textarea::-webkit-input-placeholder {color: #cacaca;}
.joinIn .bot li{width:100%;height:1.1rem;padding:0 0.3rem;border-bottom:1px solid #e4e4e4;background-color:#fff;font-size:0.26rem;}
.joinIn .bot li .div_top{height:0.4rem;overflow: hidden;}
.joinIn .bot li .p_top{line-height:0.5rem;font-size:0.22rem;color:#b6b5b6;display:none;}
.joinIn .bot li .div_bot{height:0.66rem;line-height:0.66rem;}
.joinIn .bot li .p_city{width:95%;float:left;color:#212a36;font-size:0.28rem;color:#626262;}
.joinIn .bot li span{float:right;font-size:0.35rem;}
.joinIn .bot li.citys{height:1.6rem;}
.joinIn .bot li.com_li{padding:0;height:auto;}
.joinIn .bot li.com_li .div_top{padding:0 0.3rem;}
.joinIn .bot li.com_li .div_bot{padding:0 0.3rem;}
.joinIn .bot li .bot_tab{display:none;width:100%;height:1rem;background-color:#f2f3f7;padding:0.18rem 0;}
.joinIn .bot li .bot_tab span{display:inline-block;font-size:0.26rem;margin-left:0.3rem;padding:0.1rem 0.4rem;float: left;border:1px solid #212a36;color:#212a36;border-radius:5px;}
.joinIn .bot li .bot_tab span.on{color:#fff;background-color:#ff7400;border-color:#ff7400;}
.joinIn .bot li.nature_li .bot_tab span{width:1.89rem;padding:0.1rem 0;text-align:center;margin-right:0.3rem;}
.joinIn .bot li.nature_li .bot_tab span:nth-child(3){margin-right:0;}
.joinIn .bot li.reason_li{height:1.6rem;}
.joinIn .sub_btn{width:100%;padding:0 0.3rem;margin-top:0.6rem;text-align: center;}
.joinIn .sub_btn .btn{display: inline-block;width:100%;height:0.8rem;line-height:0.8rem;text-align:center;border-radius:5px;background-color:#d3b580;color:#fff;}
.joinIn .sub_btn p{margin-top:0.16rem;font-size:0.2rem;color:#7e7e7e;text-align:center;}
.joinIn .sub_btn p .phone{color:#ff7400;font-size:0.26rem;}
.joinIn .sub_btn .log{display: inline-block;width:1.92rem;height:0.23rem;margin:1rem 0 0.4rem 0 ;background:url(../img/cooperation/log.png) no-repeat;background-size:100% 100%;}



.joinIn_ok{width:100%;height:100%;position:relative;background-color:#f2f3f7;}
.joinIn_ok .top{width:100%;height:2.5rem;background-color:#232124;text-align:center;font-size:0.32rem;color:#d3b580;}
.joinIn_ok .top span{display: inline-block;width:0.8rem;height:0.8rem;background:url(../img/cooperation/ok.png) no-repeat;background-size:100% 100%;margin-top:0.55rem;}
.joinIn_ok .top p{margin-top:0.2rem;letter-spacing:1px;}
.joinIn_ok .com{width:100%;height:1.78rem;padding-top:0.3rem;text-align: center;font-size:0.24rem;color:#626262;}
.joinIn_ok .com p{line-height:0.4rem;}
.joinIn_ok .com p .phone{color:#ff7400;font-size:0.26rem;}
.joinIn_ok .bot{width:100%;padding:0 0.3rem;font-size:0.3rem;margin-top:0.2rem;}
.joinIn_ok .bot a{display: inline-block;width:100%;height:0.8rem;line-height:0.8rem;text-align:center;border-radius:5px;color:#fff;background-color:#d3b580;}
.joinIn_ok .log{position:absolute;bottom:1%;left:36%;display: inline-block;width:1.92rem;height:0.23rem;background:url(../img/cooperation/log.png) no-repeat;background-size:100% 100%;}
		