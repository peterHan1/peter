.dis_none{display: none;}
*{-webkit-user-select: none;-webkit-tap-highlight-color: rgba(0,0,0,0);}
.ranking_list{font-family: PingFangSC-Regular, sans-serif;}
.ranking_list .ranking_top{width:100%;height: 5.28rem;position:relative;background: url(../img/gq_ranking_bg.png) no-repeat;-webkit-background-size: 100% 100%;background-size: 100% 100%;}
.ranking_list .ranking_top .ranking_money{font-size: 0.3rem;color: #fedf51;margin-top: 1.7rem;}
.ranking_list .ranking_top .ranking_money span{display: inline-block;padding: 0 0.25rem;line-height: 0.5rem;background-color: #9c4319;border-radius: 20px;}
.ranking_list .ranking_top .ranking_time{font-size: 0.18rem;color: #7f3003;line-height: 0.5rem;}
.ranking_list .ranking_top .ranking_data{font-size: 0.24rem;color: #ffeecf;margin-top: 1.8rem;}
.ranking_list .list_box{background-color: #fca029;padding:0 0.3rem 0.4rem;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.ranking_list .list_box div{background-color: #fee9cc;padding-bottom: 0.2rem;padding-top: 0.1rem;-webkit-border-radius: 0 0 10px 10px;}
.ranking_list .list_box table{border-collapse:collapse;width: 100%;}
.ranking_list .list_box table th{font-size: 0.2rem;color: #bb7e57;line-height: 0.53rem;background-color: #ffeecf;font-weight: normal;}
.ranking_list .list_box table th:nth-child(2){text-align: center;}
.ranking_list .list_box table td{font-size: 0.2rem;color: #8d3307;}

.ranking_list .list_box table td:nth-child(1){text-align: center;width: 10%;position: relative;}
.ranking_list .list_box table td:nth-child(2){text-align: left;width: 10%;padding-left:0.35rem;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.ranking_list .list_box table td:nth-child(3){text-align: left;width: 10%;padding-left: 0.4rem;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.ranking_list .list_box table td:nth-child(4){text-align: right;width: 15%;padding-right: 0.5rem;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}


.ranking_list .list_box table tr:nth-child(1) th{background-color: #fee9cc;}
.ranking_list .list_box table tr:nth-child(2) td{background-color: #fed78a;}
.ranking_list .list_box table tr:nth-child(4) td{background-color: #fed78a;}
.ranking_list .list_box table tr:nth-child(6) td{background-color: #fed681;}
.ranking_list .list_box table tr:nth-child(odd){line-height: 0.46rem;background-color: #fee6c4;}
.ranking_list .list_box table tr:nth-child(even){line-height: 0.53rem;background-color: #fee9cc;}

.ranking_list .list_box table tr:nth-child(2) td:nth-child(1):before{content: "";position: absolute;left: 50%;top: 10%;display: inline-block;width: 0.27rem;height: 0.37rem;background: url(../img/ranking_one.png) no-repeat;-webkit-background-size: 100% 100%;background-size: 100% 100%;-webkit-transform: translateX(-50%);-moz-transform: translateX(-50%);-ms-transform: translateX(-50%);-o-transform: translateX(-50%);transform: translateX(-50%);}
.ranking_list .list_box table tr:nth-child(3) td:nth-child(1):before{content: "";position: absolute;left: 50%;top: 10%;display: inline-block;width: 0.27rem;height: 0.37rem;background: url(../img/ranking_two.png) no-repeat;-webkit-background-size: 100% 100%;background-size: 100% 100%;-webkit-transform: translateX(-50%);-moz-transform: translateX(-50%);-ms-transform: translateX(-50%);-o-transform: translateX(-50%);transform: translateX(-50%);}
.ranking_list .list_box table tr:nth-child(4) td:nth-child(1):before{content: "";position: absolute;left: 50%;top: 10%;display: inline-block;width: 0.27rem;height: 0.37rem;background: url(../img/ranking_three.png) no-repeat;-webkit-background-size: 100% 100%;background-size: 100% 100%;-webkit-transform: translateX(-50%);-moz-transform: translateX(-50%);-ms-transform: translateX(-50%);-o-transform: translateX(-50%);transform: translateX(-50%);}

.ranking_list .ranking_explain{background-color: #fca029;padding: 0 0.3rem 0.4rem;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.ranking_list .ranking_explain .explain_list{padding:0.3rem;background: url(../img/gq_explain_bg.png) no-repeat;-webkit-background-size: 100% 100%;background-size: 100% 100%;overflow: hidden;}
.ranking_list .ranking_explain h2{font-size: 0.38rem;text-align: center;margin-bottom:0.2rem;background: linear-gradient(to bottom, #f9493e, #da281e);-webkit-background-clip: text;color: transparent;}
.ranking_list .ranking_explain h2 b{display: inline-block;width: 1.28rem;height: 2px;background-color: #ee3e32;vertical-align: middle;}
.ranking_list .ranking_explain .explain_list div{font-size: 0.22rem;padding-left:0.3rem;color: #8d3307;line-height: 0.3rem;margin-top: 0.13rem;text-align: justify;position: relative;}
.ranking_list .ranking_explain .explain_list div b{display: inline-block;width:0.12rem;height:0.12rem;border-radius: 20px;background: linear-gradient(to bottom, #ffc100, #ff7905);position: absolute;left: 0;top: 6%;}
.ranking_list .ranking_explain .explain_list div.div_bottom b{top: 20%;}
.ranking_list .ranking_explain .explain_list div.explain_num{margin-top: 0;}

.ranking_list .select_data{line-height:0.65rem;padding:0 0.2rem;font-size: 0.24rem;position: absolute;left: 51%;bottom: 0.3rem;-webkit-transform: translateX(-50%);-moz-transform: translateX(-50%);-ms-transform: translateX(-50%);-o-transform: translateX(-50%);transform: translateX(-50%);}
.ranking_list .select_data span{display: inline-block;color: #ffeecf;}
.ranking_list .select_data b{display: inline-block;margin-left:2px;height: 0px;width: 0px;border-top: 6px solid #ffeecf;border-right: 6px solid transparent;border-left: 6px solid transparent;}
.ranking_list .select_data2 b{transform:rotate(180deg);-ms-transform:rotate(180deg);-moz-transform:rotate(180deg);-webkit-transform:rotate(180deg);-o-transform:rotate(180deg);}
.ranking_list .calendar{font-size: 0.2rem;height: 5rem;padding:0 0.3rem 0.2rem;z-index: 9999;width:100%;position: absolute;left: 0;top: 4.96rem;overflow: hidden;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.ranking_list .calendar ul{width: 100%;height: 100%;overflow: auto;display: inline-block;z-index: 9;-webkit-overflow-scrolling: touch;background-color: #fff;}
.ranking_list .calendar ul li{text-align: center;line-height: 0.8rem;font-size: 0.24rem;color: #000;border-bottom: 1px solid #E8E8E8;}
.ranking_list .calendar ul li:last-child{border: none;}
.ranking_list .calendar ul li.time_on{background-color: #F0F0F7;}
