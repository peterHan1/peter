	<script type="text/javascript">
		$(function(){
			$('ul li div').tap(function(){
				if($(this).next().css('display')=='block'){
					$(this).find('p').eq(1).removeClass('adclass1')
					$(this).next().hide();
				}else{
					
					$(this).find('p').eq(1).addClass('adclass1')
					$(this).next().show();
				}
					
			})

			$('.contact').tap(function(){
				layer.open({
			    content: $('#footer_tel').html(),
			    btn: ['取消'],
			    className: 'footer_tel'   
			    ,skin: 'footer'
			   
			  });
			})

			 
		})
	</script>
