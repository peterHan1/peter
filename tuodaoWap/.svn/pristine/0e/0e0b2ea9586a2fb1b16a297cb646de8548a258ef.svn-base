html,body{font-family: PingFangSC-Regular, sans-serif;}
.dis_none{display: none;}
.coupon{width: 100%;height:19.38rem;position:relative;background: url(../image/coupon_bg.png) no-repeat;-webkit-background-size: 100% 100%;background-size: 100% 100%;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.coupon .count_down{
	position: fixed;
	left:0;
	top:0;
	right:0;
	font-size:0.3rem;
	color: #FFF5E6;
	height: 0.74rem;
	line-height: 0.74rem;
	background-color: #F9481F;
	text-align: center;
	overflow: hidden;
	z-index: 999;
}
.coupon .count_down .count_top{
	position: relative;
	display: inline-block;
	padding-left:0.4rem
}
.coupon .count_down .count_txt{
	display: inline-block;
}
.coupon .count_down b{
	font-weight: normal;
	position: relative;
	padding-left: 0.45rem;
}
.coupon .count_down b:after{
	content: "";
	position: absolute;
	left: 0;
	top: 10%;
	width: 0.28rem;
	height: 0.32rem;
	background: url(../image/alarm_bg.png) no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.coupon .count_down div span{
	display: inline-block;
	width:0.36rem;
	height: 0.42rem;
	line-height: 0.42rem;
	background-color:#E60B14;
	color: #fff;
	margin-right: 0.08rem;
	border-radius: 2px;
	font-weight: bold;
}
.coupon .btn_box{
	text-align: center;
	position: absolute;
	top: 7.56rem;
	left: 50%;
	-webkit-transform: translateX(-50%) ;
	-moz-transform: translateX(-50%) ;
	-ms-transform: translateX(-50%) ;
	-o-transform: translateX(-50%);
	transform: translateX(-50%);
}
.coupon .get_btn div{
	display: inline-block;
	width: 2.6rem;
	height: 2.6rem;
}
.coupon .get_btn div.btn_no{
	background: url(../image/get_btn.png) no-repeat;
	background-position: 0 0;
	background-size: 5rem 2.6rem;
}
.coupon .get_btn div.btn_ok{
	background: url(../image/get_btn.png) no-repeat;
	background-position: -2.5rem 0;
	background-size: 5rem 2.6rem;
	cursor: pointer;
}
.coupon .get_btn p{
	font-size: 0.3rem;
	color: #fff;
	white-space: nowrap;
}
.coupon .get_btn p span{
	font-size:0.46rem;
	color:#FFF54E;
}
.coupon .get_yet{
	margin-top: 0.6rem;
	line-height: 0;
}
.coupon .get_yet p{
	font-size: 0.3rem;
	line-height: 0.36rem;
	white-space: nowrap;
}
.coupon .get_yet p:nth-child(1){
	color: #fff;
}
.coupon .get_yet p:nth-child(2){
	color: #A81B25;
	margin-top: 0.39rem;
}
.coupon .get_yet .three_btn{
	display: inline-block;
	width: 4.08rem;
	height: 0.82rem;
	line-height: 0.82rem;
	color: #ef2432;
	font-size: 0.42rem;
	margin-top: 0.25rem;
	border-radius: 20px;
	box-shadow: 0px 5px 0px #f8983c, 0px 6px 20px rgba(0, 0, 0, .5);
	font-weight: bold;
	background: -webkit-linear-gradient(bottom, #ffec69 , #fec026);
	background: -o-linear-gradient(bottom, #ffec69, #fec026);
	background: -moz-linear-gradient(bottom, #ffec69, #fec026);
	background: linear-gradient(to bottom, #ffec69 , #fec026);
	cursor: pointer;
}
.explain_list{
	position: absolute;
	bottom:1rem;
	left:0;
	padding:0 0.75rem 0 0.95rem;
}
.explain_list li{
	font-size:0.28rem;
	color:#fff;
	line-height: 0.42rem;
	text-align: justify;
	position: relative;
}
.explain_list li:before{
	content: "";
	display: inline-block;
	position: absolute;
	left:-0.3rem;
	width:0.12rem;
	height:0.12rem;
	background-color: #FFF0AE;
	border-radius: 50%;
}
.explain_list li:nth-child(1):before,li:nth-child(4):before,li:nth-child(6):before{
	top:39%;
}
.explain_list li:nth-child(2):before,li:nth-child(3):before{
	top:19%;
}
.explain_list li:nth-child(5):before{
	top:12%;
}
.layer_box .shada{
	position: fixed;
	left:0;
	top:0;
	right:0;
	bottom:0;
	background-color: rgba(0,0,0,0.5);
}
.layer_box .layer_con{
	text-align: center;
	z-index: 99;
	width:5.7rem;
	height:5.3rem;
	position: fixed;
	top:50%;
	left:50%;
	-webkit-transform: translateY(-50%) translateX(-50%);
	-moz-transform: translateY(-50%) translateX(-50%);
	-ms-transform: translateY(-50%) translateX(-50%);
	-o-transform: translateY(-50%) translateX(-50%);
	transform: translateY(-50%) translateX(-50%);
}
.layer_box .layerOk{
	background:url(../image/layer_okBg.png) no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.layer_box .layerEnd{
	background:url(../image/layer_endBg.png) no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.layer_box .layerLogin{
	background:url(../image/layer_loginBg.png) no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.layer_box .layer_btn{
	position: absolute;
	bottom:0.7rem;
	left: 50%;
	font-size: 0.42rem;
	color: #cc272d;
	-webkit-transform: translateX(-50%) ;
	-moz-transform: translateX(-50%) ;
	-ms-transform: translateX(-50%) ;
	-o-transform: translateX(-50%);
	transform: translateX(-50%);
	display: inline-block;
	width: 2.95rem;
	height: 0.76rem;
	line-height: 0.76rem;
	border-radius: 20px;
	box-shadow: 0px 5px 0px #f7a73b, 0px 6px 12px rgba(0, 0, 0, .5);
	background: -webkit-linear-gradient(right, #fae04b , #ffc73d);
	background: -o-linear-gradient(right, #fae04b, #ffc73d);
	background: -moz-linear-gradient(right, #fae04b, #ffc73d);
	background: linear-gradient(to right, #fae04b , #ffc73d);
	
}
.layer_box .close{
	width:0.72rem;
	height:0.72rem;
	background:url(../image/close.png) no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
	position: absolute;
	left:50%;
	bottom:-1.2rem;
	-webkit-transform: translateX(-50%) ;
	-moz-transform: translateX(-50%) ;
	-ms-transform: translateX(-50%) ;
	-o-transform: translateX(-50%);
	transform: translateX(-50%);
}
/*落地页*/
