.layer_box .layerRule{
	width: 5.3rem;
	height: auto;
	font-size: 0.28rem;
	color: #a16b35;
	padding: 0.9rem 0.3rem 0.45rem;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	background-color: #f6ae66;
	border-radius: 20px;
	z-index: 99;
	position: fixed;
	top: 46%;
	left:50%;
	-webkit-transform: translateY(-50%) translateX(-50%);
	-moz-transform: translateY(-50%) translateX(-50%);
	-ms-transform: translateY(-50%) translateX(-50%);
	-o-transform: translateY(-50%) translateX(-50%);
	transform: translateY(-50%) translateX(-50%);

}
.layer_box .layerRule .layer_tltBg{
	position: absolute;
	left: -0.3rem;
	right: -0.3rem;
	top: -0.3rem;
	width: 5.88rem;
	height: 1.19rem;
	background: url(../image/load/layer_tlt.png) no-repeat;
	-webkit-background-size: 100% 100%;
	background-size: 100% 100%;
}
.layer_box .layerRule .layerRule_list{
	/*height: 6.8rem;*/
	/*overflow-x: scroll;*/
	/*-webkit-overflow-scrolling: touch;*/
}
.layer_box .layerState .layerRule_list{
	height: 5.2rem;
	overflow-x: auto;
}

.layer_box .layerRule p{
	line-height: 0.4rem;
	border-bottom: 1px solid #E99D50;
	padding-bottom: 0.27rem;
	text-align: justify;
}
.layer_box .layerRule h5{
	font-weight: normal;
	margin: 0.25rem 0 0.08rem;
	font-size: 0.28rem;
}
.layer_box .layerState h5{
	margin: 0rem 0 0.08rem;	
}
.layer_box .layerRule li{
	line-height: 0.4rem;
	margin-top: 0.1rem;
	padding-left: 0.23rem;
	position: relative;
}
.layer_box .layerRule li:before{
	content: "";
	display: inline-block;
	position: absolute;
	top: 16%;
	left:0;
	width:0.12rem;
	height:0.12rem;
		background-color: #a16b35;
		border-radius: 50%;
	}
	.layer_box .layerRule li:nth-child(2):before{
		top:5%;
	}
	.layer_box .layerRule li:nth-child(3):before{
		top:15%;
	}
	.layer_box .layerRule li:nth-child(4):before{
		top:10%;
	}
	.layer_box .layerRule li:nth-child(5):before{
		top:26%;
	}
	.layer_box .layerState li:nth-child(1):before{
		top: 10%;
	}
	.layer_box .layerState li:nth-child(2):before{
		top: 20%;
	}
	.layer_box .layerState li:nth-child(3):before{
		top:5%;
	}
	.layer_box .layerState li:nth-child(4):before{
		top:26%;
	}
