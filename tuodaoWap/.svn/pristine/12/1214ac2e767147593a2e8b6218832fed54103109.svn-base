html,body{height:100%;overflow: hidden;}
*{-webkit-overflow-scrolling: touch;box-sizing:border-box; font-family:"Microsoft YaHei";}
input,textarea{-webkit-appearance: normal; -moz-appearance: normal; -o-appearance: normal; appearance: normal;border:none;outline:none}
.dis_none{display: none;}
.fl{float:left;}
.fr{float:right;}
.loan{height:100%;background-color:#f2f3f7;overflow: hidden;position: relative;}
.loan_box{width:200%;height: 100%;overflow: hidden;transition: all 0.5s;-moz-transition: all 0.5s;-webkit-transition: all 0.5s;-o-transition: all 0.5s;position: absolute;top: 0;left: 0;}
.loan_list{width:50%;}
.loan .top{width:100%;margin-top:0.2rem;}
.loan .loan_list li{width:100%;height:1.1rem;padding:0 0.3rem;position:relative;border-bottom:1px solid #e4e4e4;overflow:hidden;background-color:#fff;font-size:0.26rem;}
.loan .top li:nth-child(3){border:none;}
.loan .loan_list li .div_top{height:0.4rem;overflow: hidden;}
.loan .loan_list li .p_top{line-height:0.5rem;font-size:0.22rem;color:#ff7400;display: none;}
.loan .loan_list li .div_bot{height:0.66rem;line-height:0.66rem;}
.loan .loan_list li .div_bot input{width:100%;height:100%;font-size:0.28rem;color:#212a36;caret-color:#ff7400;}
.loan .loan_list li .div_bot div{position:absolute;right:0;top:0.4rem;width:0.24rem;height:0.24rem;margin:0.2rem;background:url(../img/cooperation/x.png) no-repeat;background-size:100% 100%;z-index: 999;}
.loan .loan_list li .div_bot .code_inp{width:70%;float: left;}
.loan .loan_list li .div_bot .code_btn{display: inline-block;width:1.8rem;height:0.6rem;font-size:0.22rem;line-height:0.6rem;text-align: center;background-color:#e4e4e4;border-radius: 5px;float: right;}
.loan .loan_list li .tex_bot{height:0.96rem;margin-top:0.18rem;}
.loan .loan_list li .tex_bot textarea{width:100%;height:100%;resize: none;outline: none;border: none;font-size:0.28rem;color:#212a36;caret-color:#ff7400;}
.loan .loan_list li .p{width: 92%;float: left;color: #626262;font-size: 0.28rem;}
.loan .loan_list li .iconfont{float: right;font-size: 0.2rem;}
.loan .txt{line-height:0.6rem;font-size:0.2rem;color:#7e7e7e;padding:0 0.3rem;}
.loan .div_btn{width:100%;padding:0 0.3rem;margin-top:0.6rem;text-align: center;overflow: hidden;}
.loan .div_btn .btn{display:block;width:100%;height:0.8rem;font-size:0.4rem;line-height:0.8rem;text-align:center;border-radius:5px;background-color:#cacaca;color:#fff;}
.loan .div_btn .sub_btn{background-color:#d3b580;}
.loan .div_btn p{margin-top:0.16rem;font-size:0.2rem;color:#7e7e7e;text-align:center;}
.loan .div_btn p .phone{color:#ff7400;font-size:0.26rem;}
.loan .div_btn .log{display: inline-block;width:1.92rem;height:0.23rem;margin:2rem 0 0.4rem 0 ;background:url(../img/cooperation/log.png) no-repeat;background-size:100% 100%;}
.inp_bot input::-webkit-input-placeholder{color: #cacaca;}
.inp_bot input:-moz-placeholder{color:#cacaca;}
.inp_bot input::-moz-placeholder{color:#cacaca;}
.inp_bot input:-ms-input-placeholder{color:#cacaca;}
.inp_bot textarea::-webkit-input-placeholder {color: #cacaca;}
.layui-m-layersection .layui-m-layer-phone_show{width:5.1rem;font-size: 0.3rem;background-color:#fff;}
.layui-m-layersection .layui-m-layer-phone_show p{line-height:1.46rem;text-align: center;}
.layui-m-layer-phone_show .layui-m-layercont{padding:0;}
.phone_btn{width:100%;border-top:1px solid #e4e4e4;overflow: hidden;}
.phone_btn span{display: inline-block;width:45%;line-height:0.8rem;text-align:center;}
.phone_btn span:nth-child(1){border-right: 1px solid #e4e4e4;cursor:pointer}
.phone_btn span a{display: inline-block;width:100%;height:100%;color:#ff7400;}
/*申请弹窗*/
.layui-m-layer-mess_show .layui-m-layercont{height:1rem;width:3.5rem;background-color:#fff;color:#000;}
.layui-m-layer-mess_show .layui-m-layerbtn{height:0.8rem;line-height:0.8rem;background-color:#fff;}

/*车辆品牌选择、城市选择*/
.loan_box .nav{width:100%;height:1rem;line-height:1rem;padding:0 2%;text-align:center;border-bottom:1px solid #EDEDED;z-index:999;position:fixed;top:0;background-color:#fff;}
.loan_box .nav h3{display: inline-block;font-weight: normal;font-size:0.3rem;}
.loan_box .nav span{color:#999;}
.car_list_box,.city_list_box{width:50%;height:100%;font-size:0.2rem;overflow: hidden;}
.car_list_box .car_list,.city_list_box .city_list{position: fixed;top: 1rem;bottom: 0;width: 100%;height:100%;z-index: 99;}
.car_list_box .car_list .cities .title {height: 28px;padding-left: 5%;line-height: 28px;background-color: #f5f5f5;font-family: Helvetica;font-size: 0.22rem;color: #878787;}
.car_list_box .car_list .cities .item {z-index: 99999;height: 44px;padding: 0 5%;line-height: 44px;font-size: 0.26rem;background-color: #fff;border-bottom: 1px solid #ededed;}
.car_list_box .car_list .cities .onli {background-color: #ededed;}
.car_list_box .car_list .cities .item .name {display: block;position: relative;}
.car_list_box .car_list .cities .item .name:before,
.car_list_box .car_list .cities .item .name:after {display: block;position: absolute;border-top: 1px solid #e5e5e5;left: 0;width: 100%;content: ' ';}
.car_list_box .car_list .cities .item .name:before {display: none;top: 0;}
.car_list_box .car_list .cities .item .name:after {display: block;bottom: 0;}
.car_list_box .car_list .cities .item:active {background-color: #f0f0f0;}
.car_list_box .car_list .cities .item:last-child .name:after {display: none;}
.car_list_box .car_list .shortcut {position: absolute;width: 0.8rem;right: 0;font-family: Helvetica;z-index: 9999;}
.car_list_box .car_list .shortcut .item {height: 0.42rem;line-height:0.32rem;padding-left:0.2rem;padding-top:0.1rem;text-align: center;font-size: 0.32rem;color: #fa8919;}
.car_list_box .car_list .shortcut .jump_tips {position: fixed;top: 45%;right: 36%;z-index: 100;display: none;width: 1.4rem;padding: 0.5rem 0rem;font-size: 0.5rem;color: #fff;text-align: center;line-height: 1;background-color: rgba(0, 0, 0, 0.8);}
.car_list_box .car_list .scroller_hook {padding-bottom: 1rem;}
.car_list_box .list_one,.car_list_box .list_two{width:100%;height:100%;}
#car_list_botul{height: 100%;width:100%;padding-bottom:1.5rem;}
#car_list_botul ul{overflow-y:auto;-webkit-overflow-scrolling : touch; }
.car_list_box .list_two ul li {z-index: 99999;height: 44px;padding: 0 16px;line-height: 44px;font-size: 0.16rem;background-color: #fff;cursor:pointer;border-bottom: 1px solid #ededed;}
.car_list_box .list_two ul .onli{background-color: #ededed;}
.car_list_box .car_nav{width:100%;height:1rem;line-height:1rem;padding:0 2%;text-align:center;border-bottom:1px solid #EDEDED;background-color:#fff;}
.car_list_box .car_nav h3{display: inline-block;font-weight: normal;font-size:0.3rem;}
.car_list_box .car_nav span{color:#999;}

.city_list .city_nav{height:0.5rem;line-height: 0.5rem;border-bottom: 1px solid #ededed;background-color:#F2F2F2;}
.city_list .city_nav div{width:50%;float:left;text-align:center;}
.city_list .city{width:100%;height:100%;overflow: hidden;}
.city_list .city .city_left,.city_right{width:50%;float: left;height:100%;}
.city_list .city .city_left{width:49.5%;border-right:1px solid #ededed;}
.city_list .city .city_right{position: relative;overflow-y:auto;}
#city_ul{position: absolute;width:100%;height:100%;background-color:#fff;}
.province_ul{padding-bottom:1.5rem;}
.province_ul li{height:1rem;line-height: 1rem;text-align:center;background-color:#F2F2F2;}
.province_ul li.onli{background-color:#fff}
.city_ul li{height:1rem;line-height: 1rem;text-align:center;background-color:#fff;border-bottom: 1px solid #ededed;}
.city_ul{padding-bottom:1.5rem;position: absolute;width:100%;}
.city_ul li.onli{background-color:#ededed;}




