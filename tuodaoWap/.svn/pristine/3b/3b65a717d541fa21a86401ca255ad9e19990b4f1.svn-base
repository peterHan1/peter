body{background-color:#f3f3f3;}
.text_top{width:92%;background-color:#f3f3f3;font-size:0.24rem;color:#777;padding:3% 4%;line-height:0.36rem;}
.select_bank .ul_bank ul li{border-bottom:1px solid #d8d8d8;width:94%;position:relative;padding:1% 3%;background-color:white;overflow: hidden;}
.select_bank .ul_bank ul li div{height:0.7rem;float:left;line-height:0.4rem;padding-top:0.14rem;}
.select_bank .ul_bank ul li div h4{font-size:0.26rem;color:#333;}
.select_bank .ul_bank ul li div p{font-size:0.24rem;color:#777;}
.select_bank .ul_bank ul li span{display:inline-block;width:1rem;height:1rem;float:left;margin-right:0.2rem;background:url(../images/bank_logo.png) no-repeat;background-size:5rem 4rem;}
.select_bank .ul_bank ul .on{background:#fff url(../images/yes.png) no-repeat 95% 50%;background-size:7%;}
#bank_ul .ICBC{background-position:0.0rem 0rem;}
#bank_ul .CCB{background-position:-1rem 0rem;}
#bank_ul .BOC{background-position:-2rem 0rem;}
#bank_ul .ABC{background-position:-3rem 0rem;}
#bank_ul .CMB{background-position:-4rem 0rem;}
#bank_ul .CITIC{background-position:0rem -1rem;}
#bank_ul .SPDB{background-position:-1rem -1rem;}
#bank_ul .CIB{background-position:-2rem -1rem;}
#bank_ul .CMBC{background-position:-3rem -1rem;}
#bank_ul .CEB{background-position:-4rem -1rem;}
#bank_ul .PAB{background-position:0rem -2rem;}
#bank_ul .HXB{background-position:-1rem -2rem;}
#bank_ul .BOB{background-position:-2rem -2rem;}
#bank_ul .CGB{background-position:-3rem -2rem;}
#bank_ul .NBCB{background-position:-4rem -2rem;}
#bank_ul .HZB{background-position:0rem -3rem;}
#bank_ul .BOS{background-position:-1rem -3rem;}
#bank_ul .PSBC{background-position:-2rem -3rem;}
#bank_ul .BCM{background-position:-3rem -3rem;}
