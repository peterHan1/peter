/*加载中*/
.reg{height:100%;overflow: hidden;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;margin:0;padding-top:0.2rem}
.reg .spinner_box{position: absolute;top: 50%;left:50%;width:3.4rem;height:2.8rem;transform:translateX(-50%) translateY(-50%);background-color:#f8f8f8;border:1px solid #e4e4e4;border-radius: 5px;}
.reg .spinner {margin: 0.5rem auto;width: 0.85rem;height: 0.85rem;position: relative;}
.reg .spinner_box .spinner_txt{font-size:0.28rem;color:#7E7E7E;text-align: center;}
.reg .container1 > div, .container2 > div, .container3 > div {width: 0.2rem;height: 0.2rem;background-color: #7e7e7e;border-radius: 100%;position: absolute;-webkit-animation: bouncedelay 1.2s infinite ease-in-out;animation: bouncedelay 1.2s infinite ease-in-out;-webkit-animation-fill-mode: both;animation-fill-mode: both;}
.reg .spinner .spinner-container {position: absolute;width: 100%;height: 100%;}
.reg .container2 {-webkit-transform: rotateZ(45deg);transform: rotateZ(45deg);}
.reg .container3 {-webkit-transform: rotateZ(90deg);transform: rotateZ(90deg);}
.reg .circle1 { top: 0; left: 0; }
.reg .circle2 { top: 0; right: 0; }
.reg .circle3 { right: 0; bottom: 0; }
.reg .circle4 { left: 0; bottom: 0; }
.reg .container2 .circle1 {-webkit-animation-delay: -1.1s;animation-delay: -1.1s;}
.reg .container3 .circle1 {-webkit-animation-delay: -1.0s;animation-delay: -1.0s;}
.reg .container1 .circle2 {-webkit-animation-delay: -0.9s;animation-delay: -0.9s;}
.reg .container2 .circle2 {-webkit-animation-delay: -0.8s;animation-delay: -0.8s;}
.reg .container3 .circle2 {-webkit-animation-delay: -0.7s;animation-delay: -0.7s;}
.reg .container1 .circle3 {-webkit-animation-delay: -0.6s;animation-delay: -0.6s;}
.reg .container2 .circle3 {-webkit-animation-delay: -0.5s;animation-delay: -0.5s;}
.reg .container3 .circle3 {-webkit-animation-delay: -0.4s;animation-delay: -0.4s;}
.reg .container1 .circle4 {-webkit-animation-delay: -0.3s;animation-delay: -0.3s;}
.reg .container2 .circle4 {-webkit-animation-delay: -0.2s;animation-delay: -0.2s;} 
.reg .container3 .circle4 {-webkit-animation-delay: -0.1s;animation-delay: -0.1s;}
@-webkit-keyframes bouncedelay {0%, 80%, 100% { -webkit-transform: scale(0.0) }40% { -webkit-transform: scale(1.0) }}
@keyframes bouncedelay {0%, 80%, 100% {transform: scale(0.0);-webkit-transform: scale(0.0);} 40% {transform: scale(1.0);-webkit-transform: scale(1.0);}}


