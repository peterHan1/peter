.dis_none{display: none;}
/*login start*/
.main{margin-top: 0.2rem;position: relative;}
.main input{outline: none;border: none;}
.main .pswIcon{position: absolute;top: 0.3rem;right: 0.3rem;font-size: 0.28rem;color: #ff7400;padding:0 0.1rem;}
.main .phone{box-sizing:border-box;-moz-box-sizing:border-box;display: block;padding-left: 0.3rem;padding-right: 0.7rem;width: 100%;height: 1.1rem;line-height: 1.1rem;font-size: 0.28rem;color: #626262;}
.main .padding{box-sizing:border-box;-moz-box-sizing:border-box;width: 100%;padding: 0rem 0.3rem;margin-top: 0.6rem;}
.main .padding .next{display: block;clear:both;width: 100%;height: 0.8rem;line-height: 0.8rem;color: #ffffff;text-align: center;font-size: 0.3rem;color: #ffffff;border-radius: 0.05rem;background: #cacaca;}
.investUser{box-sizing:border-box;-moz-box-sizing:border-box;position: relative;height: 100%;padding-top: 1rem;}
.investUser .investUser_top{width: 100%;box-sizing:border-box;-moz-box-sizing:border-box;padding:0 0.2rem;color: #626262;font-size: 0.28rem;}
.investUser .investUser_bottom{width: 100%;font-size: 0.3rem;text-align:center;color: #626262;height: 1rem;line-height: 1rem;border-top: 1px solid #626262;position: absolute;bottom: 0;}
.main .padding .forget{float: right;color: #ff7100;font-size: 0.2rem;margin-top: 0.2rem;text-align: center;}
.main .border{overflow: hidden;background:#ffffff;border-bottom: 1px solid #f2f3f7;position: relative;width: 100%}
.main .border .ipt{box-sizing:border-box;-moz-box-sizing:border-box;display: block;padding-left: 0.3rem;padding-right: 2rem;width: 100%;height: 1.09rem;line-height: 1.09rem;font-size: 0.28rem;color: #626262;}
.main .border p{position: absolute;top: 0rem;right: 0.3rem;color: #ff7400;font-size: 0.24rem;height: 1.09rem;line-height: 1.09rem;}
.main .border .check{width: 1.66rem;height: 0.52rem;position: absolute;top: 50%;margin-top:-0.26rem;right: 0.3rem;}
.main .border .tel{box-sizing:border-box;-moz-box-sizing:border-box;padding-left: 0.3rem;padding-right: 2rem;width: 100%;height: 1.09rem;line-height: 1.09rem;font-size: 0.28rem;color: #626262;}
.main .padding .findPsw{color: #626262;font-size: 0.2rem;margin-top: 0.2rem;text-align: center;}
.main .padding .findPsw span{color: #ff7400;font-size: 0.2rem;}
/*login end*/
/*cash start*/
.cash{position: relative;height:100%;}
.cash .cash_list p{line-height:0.8rem;padding:0 0.3rem;font-size:0.24rem;color:#7e7e7e;}
.cash .cash_list li{overflow:hidden;padding:0 0.3rem;line-height:0.9rem;font-size:0.28rem;border-bottom: 1px solid #e4e4e4;background-color:#fff;}
.cash .cash_list li div:nth-child(1){float:left;color:#212a36;}
.cash .cash_list li div:nth-child(2){float:right;color:#7e7e7e;}
.cash .btn{padding:0 0.3rem;margin-top:0.6rem;font-size:0.3rem;}
.cash .btn a{width:100%;color:#fff;text-align:center;border-radius:3px;line-height:0.8rem;display: inline-block;}
.cash .btn .no_btn{background-color:#cacaca;}
.cash .btn .subBtn{background-color:#ff711c;}
.cash .btn p{text-align:center;font-size:0.22rem;color:#ff1c1c;line-height:0.6rem;}
.cash .keywords{width:100%;position:absolute;bottom:0;z-index: 999;display:none;}
.cash .keywords .psw{width:100%;height:3.75rem;background-color:#fff;}
.cash .keywords .psw p{font-size:0.32rem;color:#626262;padding:0 0.3rem;line-height:1rem;text-align:center;border-bottom:1px solid #e4e4e4;}
.cash .keywords .psw p .iconfont{color:#b2b5b6;font-size:0.4rem;}
.cash .keywords .psw a{line-height:0.8rem;font-size:0.24rem;color:#ff7400;margin-right:0.3rem;}
.cash .keywords .psw .inputPsw{padding:0 0.3rem;margin-top:0.5rem;}
.cash .keywords .psw .inpPsw{border:1px solid #cacaca;border-radius: 3px;overflow: hidden;}
.cash .keywords .psw .inpPsw span{display:inline-block;float:left;width:16.6%;height:1.1rem;text-align:center;border-right:1px solid #cacaca;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.cash .keywords .psw .inpPsw span b{width:0.2rem;height:0.2rem;display:block;margin:38% auto;border-radius:50%;background-color:#212a36;display:none;}
.cash .keywords .psw .inpPsw span:last-child{border:none;}
.cash .keywords .keyNumber{width:100%;}
.cash .keywords .keyNumber li{border-top:1px solid #8c8c8c;overflow: hidden;}
.cash .keywords .keyNumber li div{width:33.3%;height:1.1rem;float: left;text-align: center;color:#000;background-color:#fff;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.cash .keywords .keyNumber li div:nth-child(2){border-left:1px solid #8c8c8c;border-right:1px solid #8c8c8c;}
.cash .keywords .keyNumber li div p:nth-child(1){font-size:0.48rem;margin-top:0.08rem;}
.cash .keywords .keyNumber li div p:nth-child(2){font-size:0.22rem;margin-top:-0.08rem;}
.cash .keywords .keyNumber li div.on{background-color:#d1d5db;}
.cash .keywords .keyNumber .keyBottom div p:nth-child(1){line-height:1.1rem;margin:0;}
.cash .keywords .keyNumber li .bg_color{background-color:#d1d5db;}
.cash .keywords .keyNumber li .bg_color .iconfont{font-size:0.6rem;}
.cash .shade{position:absolute;left:0;top:0;bottom:0;right:0;width:100%;height:100%; background-color: #000;filter:alpha(opacity=50);-moz-opacity:0.5;-khtml-opacity: 0.5; opacity: 0.5;z-index: 99;display:none;}
.cash .loading{width:100%;height:6.4rem;background-color:#fff;position: absolute;bottom: 0;z-index: 999;text-align:center;display: none;padding-top:1.8rem}
.cash .loading div{width:1.3rem;height:1.3rem;box-sizing: border-box;display: inline-block;}
.cash .loading p{font-size:0.28rem;color:#212a36;line-height:0.88rem;}
.cash .loading .cash_ok{background: url(../image/cash_ok.png) no-repeat;background-size:100% 100%;}
.cash .loading .cash_no{background: url(../image/cash_no.png) no-repeat;background-size:100% 100%;}
.cash .loading .cash_loading{background: url(../image/cash_loading.png) no-repeat;background-size:100% 100%;}
.cash .loading .applying{width:1.3rem;height:1.3rem;border:0.1rem solid #ff7400;border-right-color:#fff;border-radius:50%;-webkit-animation:circle 1s infinite linear;}
@-webkit-keyframes circle{
	0%{ transform:rotate(0deg); }
	100%{ transform:rotate(-360deg);}
}
.cash .explain{height:100%;background-color:#fff;}
.cash .explain .tilte{border-bottom: 1px solid #e4e4e4;}
.cash .explain .explain_box{padding:0 0.3rem;}
.cash .explain h1{font-size:0.32rem;color:#212a36;font-weight:normal;line-height:0.8rem;}
.cash .explain p{font-size:0.26rem;color:#626262;line-height:0.5rem;margin-bottom: 0.1rem;}
/*cash end*/
/*myCenter start*/
.myBottom{background-color: #fff;height:.92rem;width:100%;border-top:1px solid #cdcdcd;position: fixed;bottom:0;}
.myBottom ul{width:100%;height:100%;}
.myBottom ul li{width:50%;height:100%;line-height:.92rem;text-align: center;float:left;color:#626262;}
.myBottom ul li.on{color:#ff711c;}
.myBottom ul li dt{font-size:.4rem;height:.3rem;line-height:.3rem;margin-top:.15rem;margin-bottom:.1rem;}
.myBottom ul li dd{font-size:.16rem;height:.3rem;line-height: .3rem;}
.myCenter .no_data{position: absolute;left:50%;top:50%;margin-left:-0.67rem;margin-top:-0.654rem;font-size:.2rem;color:#b6b5b6;text-align: center;}
.myCenter .no_data .logo{width:1.34rem;height:1.29rem;background: url(../image/no_data.png) no-repeat;background-size: 100%;margin-bottom:.2rem;}
.myCenter .top_nav{height:.88rem;background-color: #fff;}
.myCenter .top_nav ul li{height:.86rem;font-size:.21rem;color:#626262;float:left;line-height: .86rem;margin:0 1.44rem;}
.myCenter .top_nav ul li.on{color:#ff7400;border-bottom:2px solid #ff7400;}
.myCenter .cash_list{width:100%;overflow: hidden;}
.myCenter .cash_list li{width:100%;height:2.68rem;background-color: #fff;margin-top:.19rem;overflow: hidden;}
.myCenter .cash_list .list{width:6.89rem;height:2rem;margin: .38rem .3rem .42rem .3rem;}
.myCenter .cash_list .list .list_top{width:100%;height:1rem;border-bottom:1px solid #e4e4e4;}
.myCenter .cash_list .list .list_top dl{width:4.89rem;height:.75rem;float:left;}
.myCenter .cash_list .list .list_top dl dt{color:#ff711c;font-size:.33rem;}
.myCenter .cash_list .list .list_top dl dd{color:#b6b5b6;font-size:.18rem;}
.myCenter .cash_list .list .list_top .btn{width:2rem;height:.64rem;background-color: #ff711c;line-height:.64rem;text-align: center;font-size:.19rem;float:left;border-radius:0.1rem;}
.myCenter .cash_list .list .list_top .btn a{color:#fff;}
.myCenter .cash_list .list .list_top .look{width:1.9rem;height:.64rem;background-color: #fff;border: 1px solid #ff711c;line-height:.64rem;text-align: center;font-size:.19rem;float:left;border-radius:0.1rem;}
.myCenter .cash_list .list .list_top .look a{color:#ff711c;}
.myCenter .cash_list .list .list_top .again{width:2rem;height:.64rem;background-color: #ff1c1c;line-height:.64rem;text-align: center;font-size:.19rem;float:left;border-radius:0.1rem;}
.myCenter .cash_list .list .list_top .again a{color:#fff;}
.myCenter .cash_list .list .list_top .del{width:2rem;height:.64rem;font-size: .21rem;color:#212a36;text-align: right;float:left;}
.myCenter .cash_list .list .list_bottom{width:100%;height:.98rem;overflow: hidden;}
.myCenter .cash_list .list .list_bottom dl{width:50%;margin-top:.32rem;float:left;}
.myCenter .cash_list .list .list_bottom dl dt{font-size:.18rem;color:#212a36;}
.myCenter .cash_list .list .list_bottom dl dd{font-size:.18rem;color:#b6b5b6;}
.myCenter .cash_list .list .list_bottom dl.right{text-align: right;}
#win_boxs{overflow: hidden;width:5.1rem;height:2.75rem;background-color: #fff;display: none;}
#win_boxs .win_top{width:4.4rem;height:1.35rem;border-bottom:1px solid #e4e4e4;font-size:.2rem;color:#212a36;text-align: left;padding: .59rem .35rem 0 .35rem;line-height:.4rem;}
#win_boxs .win_bottom {height:.79rem;font-size:.22rem;text-align: center;line-height: .79rem;}
#win_boxs .win_bottom a{color:#ff7400;}
/*我的*/
.myInfo .info_box ul{width:100%;overflow: hidden;}
.myInfo .info_box ul li{width:100%;height:.89rem;border-bottom: 1px solid #e4e4e4;background-color: #fff;}
.myInfo .info_box ul li span{display:block;font-size:.21rem;width:40%;height:100%;line-height: .89rem;}
.myInfo .info_box ul li span.left{text-align:left;color:#212a36;margin-left:.31rem;}
.myInfo .info_box ul li span.right{text-align: right; color:#7e7e7e;margin-right:.31rem;}
.myInfo .info_box ul li.border{border-bottom:.2rem solid #f2f3f7;}
.myInfo .info_box ul li p{width:100%;height:100%;line-height:.89rem;color:#ff711c;text-align: center;font-size:.21rem;}
#quit_box{width:5.11rem;height:2.27rem;overflow: hidden;background-color: #fff;display: none;}
#quit_box .quit_top{width:100%;height:1.46rem;text-align: center;line-height: 1.46rem;font-size:.21rem;color:#212a36;border-bottom:1px solid #e4e4e4;}
#quit_box .quit_bottom{width:100%;height:.79rem;font-size:.22rem;color:#626262;line-height: .79rem;}
#quit_box .quit_bottom span{display:block;text-align: center;width:49%;height:100%;float:left;}
#quit_box .quit_bottom span.stay{border-left:1px solid #e4e4e4;color:#ff7400;}
/*myCenter end*/
/*openDeposit start*/
input[type=button], input[type=submit], input[type=file] { cursor: pointer; -webkit-appearance: none; }
::-webkit-input-placeholder { /* WebKit browsers */
    color:#626262;
}
/*body{position:relative;}*/
.deposit_box{position:relative;}
.deposit .dis{background-color: #fff!important;color:#b6b5b6!important;}
.deposit .kd{background-color: #ff711c!important;}
.deposit .no-border{border-bottom:0!important;margin-bottom:.2rem;}
.deposit .notice p{width:100%;color:#626262;font-size:.16rem;width:92%;margin:.2rem auto;line-height: .4rem;}
.deposit .formBox{overflow: hidden;}
.deposit .formBox div{width:100%;height:1.12rem;background-color: #fff;border-bottom:1px solid #e4e4e4;line-height: 0;}
.deposit .formBox div input{width:100%;height:100%;border:none;outline: none;padding-left: .3rem;color:#212a36;font-size:.21rem;}
.deposit .formBox div a{display: block;width:100%;height:100%;}
.deposit .formBox div a span{display:block;width:30%;height:100%;line-height:1.12rem;font-size:.21rem;color:#626262;margin-left:.31rem;}
.deposit .formBox div a span.icon{text-align: right;margin-right:.3rem;}
.deposit .formBox .sure{height:3.83rem;background-color: #fff;}
.deposit .formBox .sure input{width:6.9rem;height:.8rem;margin:.6rem .3rem 0 .3rem;padding: 0;color:#fff;border-radius:0.05rem;background-color: #cacaca;}
.deposit .formBox .card{position: relative;}
.deposit .formBox .card .check{width:2.28rem;height:1.09rem;position: absolute;right:0;top:0;background-color: #cacaca;text-align: center;line-height:1.08rem;color:#fff;font-size:.2rem;display: none;}
.deposit .formBox .cardNum{position: relative;}
.deposit .formBox .cardNum span{display:block;position: absolute;font-size:.24rem;color:#cacaca;top:.53rem;right:.31rem;display: none;}
.select_bank{display: none;overflow: hidden;height:100%;}
.select_bank .text_top{width:92%;background-color:#f3f3f3;font-size:0.24rem;color:#777;padding:3% 4%;line-height:0.36rem;text-align: center;}
.select_bank .ul_bank ul li{border-bottom:1px solid #d8d8d8;width:94%;position:relative;padding:1% 3%;background-color:white;overflow: hidden;}
.select_bank .ul_bank ul li div{height:0.7rem;float:left;line-height:0.4rem;padding-top:0.14rem;}
.select_bank .ul_bank ul li div h4{font-size:0.26rem;color:#626262;}
.select_bank .ul_bank ul li div p{font-size:0.24rem;color:#b6b5b6;}
.select_bank .ul_bank ul li span{display:inline-block;width:1rem;height:1rem;float:left;margin-right:0.2rem;background:url(../image/bank_logo.png) no-repeat;background-size:5rem 4rem;}
.select_bank .ul_bank ul .on{background:#fff url(../image/yes.png) no-repeat 95% 50%;background-size:7%;}
.select_bank #wrapper{position:absolute;left: 0;top:0.9rem;bottom:0;right:0;overflow: hidden;}
.select_bank #wrapper #bank_ul{padding-bottom:.01rem;}
#bank_ul .ICBC{background-position:0.0rem 0rem;}
#bank_ul .CCB{background-position:-1rem 0rem;}
#bank_ul .BOC{background-position:-2rem 0rem;}
#bank_ul .ABC{background-position:-3rem 0rem;}
#bank_ul .CMB{background-position:-4rem 0rem;}
#bank_ul .CITIC{background-position:0rem -1rem;}
#bank_ul .SPDB{background-position:-1rem -1rem;}
#bank_ul .CIB{background-position:-2rem -1rem;}
#bank_ul .CMBC{background-position:-3rem -1rem;}
#bank_ul .CEB{background-position:-4rem -1rem;}
#bank_ul .PAB{background-position:0rem -2rem;}
#bank_ul .HXB{background-position:-1rem -2rem;}
#bank_ul .BOB{background-position:-2rem -2rem;}
#bank_ul .CGB{background-position:-3rem -2rem;}
#bank_ul .NBCB{background-position:-4rem -2rem;}
#bank_ul .HZB{background-position:0rem -3rem;}
#bank_ul .BOS{background-position:-1rem -3rem;}
#bank_ul .PSBC{background-position:-2rem -3rem;}
#bank_ul .BCM{background-position:-3rem -3rem;}
#window_box{width:5.1rem;height:2.71rem;background-color: #fff;display: none;}
#window_box .win_top{width:5.1rem;height:1.91rem;border-bottom: 1px solid #e4e4e4;color:#212a36;font-size: .21rem;text-align: center; line-height: 1.91rem;}
#window_box .win_bottom{width:100%;height:.78rem;}
#window_box .win_bottom div{width:49%;height:100%;text-align: center;line-height: .78rem;font-size: .22rem;color:#ff7400;float:left;}
#window_box .win_bottom .leave{border-right:1px solid #e4e4e4;}
#window_box .win_bottom .leave a{color:#626262;}
#show_success{width:2.18rem;height:2.1rem;border:1px solid #e4e4e4;border-radius:0.2rem;background-color: #f8f8f8;position: fixed;top:50%;left:50%;margin-left:-1.09rem;margin-top:-1.05rem;display: none;}
#show_success .logo{width:.84rem;height:.84rem;background: url(../image/show_success.png) no-repeat;background-size:100%;margin: .42rem auto .21rem auto;}
#show_success p{color:#7e7e7e;font-size:.23rem;text-align: center;}
/*openDeposit start*/
