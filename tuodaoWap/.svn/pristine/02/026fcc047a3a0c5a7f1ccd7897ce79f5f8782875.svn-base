body{background-color: #F2F3F7}
.investSift .title{width:100%;height:.7rem;line-height: .7rem;font-size:.28rem;color:#333333;border-bottom: 1px solid #E0E0E0;}
.investSift .title span{display: inline-block;width:.06rem;height:.24rem;background-color: #FF7102;border-radius:.04rem;margin: 0 .14rem 0 .3rem;vertical-align: middle;}
.investSift .dis_none{display: none;}
.investSift .top_nav{width:100%;height:.9rem;position: fixed;background-color: #fff;z-index: 999;top:0;left:0;}
.investSift .top_nav ul{width:100%;height:100%;overflow: hidden;}
.investSift .top_nav ul li{float: left;width:33%;height:.9rem;font-size: .28rem;color:#999999;text-align: center;line-height: .9rem;position: relative;}
.investSift .top_nav ul li.on:after {content: " ";width:0.68rem;height:.06rem;background-color: #FF7102;position: absolute;left: 36%;bottom: -1px;border-radius:3px;z-index: 5;}
.investSift .top_nav ul li.on {color:#FF7B00;}
.investSift .upload{width:100%;height:.6rem;position:absolute;top:.9rem;left:0;background-color: #F2F3F7;}
.investSift .upload p{width:100%;height:.6rem;font-size:.24rem;color:#ccc;text-align: center;line-height: .6rem;}
.investSift .upload p i{margin-right:.14rem;}
.investSift .investSift_item{width:100%;position:absolute;top:1.5rem;left:0;background-color: #fff;}
.investSift .investSift_item .loan_info .info_details{width:100%;height:1.33rem;border-bottom:.2rem solid #F2F3F7;}
.investSift .investSift_item .loan_info .info_details p{height:.37rem;font-size:.26rem;padding: 0 .3rem;}
.investSift .investSift_item .loan_info .info_details p:first-child{margin-top:.35rem;margin-bottom:.2rem;}
.investSift .investSift_item .loan_info .info_details span{display: block;float:left;width:3.45rem;height:100%;line-height: .37rem;}
.investSift .investSift_item .loan_info .info_details span i{margin-right:.47rem;color:#999999;}
.investSift .investSift_item .loan_info .info_details span b{font-weight: 200;color:#333;}
.investSift .investSift_item .item_overview{overflow: hidden;}
.investSift .investSift_item .item_overview .overview_info{width:100%;height:5.61rem;padding-top: .25rem;border-bottom:.20rem solid #F2F3F7;}
.investSift .investSift_item .item_overview .overview_info .ts{margin:0 .53rem;height:.69rem;line-height: .69rem;font-size:.20rem;color:#999999;}
.investSift .investSift_item .item_overview .overview_info ul{width:100%;height:4.91rem;border-bottom:1px solid #e0e0e0;}
.investSift .investSift_item .item_overview .overview_info li{position: relative;margin: 0 .4rem .25rem .53rem;}
.investSift .investSift_item .item_overview .overview_info li:after{content:"";display: block;position: absolute;width:.08rem;height:.08rem;border-radius: .08rem;background-color: #FF7B00;left:-.25rem;top:.18rem;}
.investSift .investSift_item .item_overview .overview_info li div{font-size:.26rem;color:#999999;}
.investSift .investSift_item .item_overview .overview_info li p{font-size:.26rem;color:#333333;}
.investSift .investSift_item .car_info{overflow: hidden;}
.investSift .investSift_item .car_info .info_details{width:100%;height:1.35rem;border-bottom:.2rem solid #F2F3F7;}
.investSift .investSift_item .car_info .info_details p{width:100%;height:.37rem;font-size:.26rem;padding: 0 .3rem;}
.investSift .investSift_item .car_info .info_details p:first-child{margin-top:.35rem;margin-bottom:.2rem;}
.investSift .investSift_item .car_info .info_details span{display: block;float:left;width:3.45rem;height:100%;line-height: .37rem;}
.investSift .investSift_item .car_info .info_details span i{margin-right:.47rem;color:#999999;}
.investSift .investSift_item .car_info .info_details span b{font-weight: 200;color:#333;}
.investSift .investSift_item .check_information{width:100%;margin-bottom: 1rem;}
.investSift .investSift_item .check_information .information_pic{width:100%;overflow-x: scroll;overflow-y: hidden;white-space:nowrap; height:2.02rem;border-bottom:.2rem solid #F2F3F7;}
.investSift .investSift_item .check_information .information_pic ul li{display: inline-block;}
.investSift .investSift_item .check_information .swiper-container img{height: auto;width: 100%;}
.investSift .investSift_item .check_information .swiper-slide {text-align: center;background: #f3f3f3; display: -webkit-box;display: -ms-flexbox;display: -webkit-flex; display: flex;-webkit-box-pack: center;-ms-flex-pack: center;-webkit-justify-content: center;justify-content: center; -webkit-box-align: center;-ms-flex-align: center; -webkit-align-items: center;align-items: center;}
.investSift .investSift_item .safeguard_step .step_info{height:3.42rem;padding: .24rem .51rem .07rem .31rem;font-size:.26rem;color:#333333;}
.investSift .investSift_item .safeguard_step .step_info p{line-height:.5rem;}
.investSift .investSift_item .money_mange .mange_info{height:1.72rem;padding: .24rem .51rem .21rem .31rem;font-size:.26rem;color:#333333;}
.investSift .investSift_item .borrow_record li{margin: 0 .3rem;height:1.37rem;border-bottom:1px solid #E0E0E0;overflow: hidden;}
.investSift .investSift_item .borrow_record li:last-child{border:none;}
.investSift .investSift_item .borrow_record li .phone{display: block;float:left;width:50%;height:100%;line-height:1.37rem;font-size:.28rem;color:#333333;}
.investSift .investSift_item .borrow_record li .phone span{display: inline-block;vertical-align: middle;width:.6rem;height:.3rem;zoom:0.5;}
.investSift .investSift_item .borrow_record li .phone .most{background: url(../img/most.png) no-repeat center;}
.investSift .investSift_item .borrow_record li .phone .first{background: url(../img/first.png) no-repeat;}
.investSift .investSift_item .borrow_record li .phone .less{background: url(../img/less.png) no-repeat;}
.investSift .investSift_item .borrow_record li .info{display: block;float:left;width:50%;height:100%;text-align: right;}
.investSift .investSift_item .borrow_record li .info dt{font-size:.28rem;color:#333;height:.4rem;line-height:.4rem;margin-bottom:.08rem;margin-top:.29rem;}
.investSift .investSift_item .borrow_record li .info dd{font-size:.22rem;color:#999999;height:.3rem;line-height:.3rem;}
