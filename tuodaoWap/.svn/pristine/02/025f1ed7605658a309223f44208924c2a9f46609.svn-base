<script src="js/jquery-2.1.0.min.js"></script>
<script src="js/p-pull-refresh.js"></script>
<script>
	$(function(){
		//	tab切换
		$('.bond .top_nav ul li').on('click', function() {
			var _index = $(this).index();
			$('.bond .top_nav ul li').removeClass('on');
			$(this).addClass('on');
			$('.bond .bondItem').hide();
			$('.bond .bondItem').eq(_index).show();
			if(_index == 0) {
				pullRefresh('#bondItem1')
			} else if(_index == 1) {
				pullRefresh('#bondItem2')
			} else {
				pullRefresh('#bondItem3')
			}
		})
		pullRefresh('#bondItem1')
		function pullRefresh(_name) {
			$(_name).pPullRefresh({
				$el: $(_name),
				$loadingEl: $('.upload'),
				callbacks: {
					pullEnd: function(){
						//调用ios方法返回
					}
				}
			});
		}
	})
</script>